{
  "containerDefinitions": [
    {
      "name": "backend-express",
      "image": "603897016348.dkr.ecr.eu-south-2.amazonaws.com/ecommerce-backend",
      "cpu": 256,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environmentFiles": [],
      "mountPoints": [],
      "volumesFrom": [],
      "ulimits": [],
      "systemControls": []
    },
    {
      "name": "frontend-react",
      "image": "603897016348.dkr.ecr.eu-south-2.amazonaws.com/ecommerce-frontend",
      "cpu": 256,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 3001,
          "hostPort": 3001,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "VITE_API",
          "value": "http://localhost/api/e-commerce"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "systemControls": []
    },
    {
      "name": "redis",
      "image": "redis:7.4-alpine",
      "cpu": 128,
      "memory": 256,
      "portMappings": [
        {
          "containerPort": 6379,
          "hostPort": 6379,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "systemControls": []
    },
    {
      "name": "nginx",
      "image": "603897016348.dkr.ecr.eu-south-2.amazonaws.com/ecommerce-nginx",
      "cpu": 256,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [],
      "mountPoints": [],
      "volumesFrom": [],
      "systemControls": []
    }
  ],
  "family": "ecommerce-task",
  "taskRoleArn": "arn:aws:iam::603897016348:role/ECSTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::603897016348:role/ECSTaskExecutionRole",
  "networkMode": "awsvpc",
  "cpu": "1024",
  "memory": "3072",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  }
}